{
    "manifest_version":1,
    "name": "SpotiAdd",
    "description": "Quick add song to your playlist on Spotify",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action":{
        "default_icon": "icon_19.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}